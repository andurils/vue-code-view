{
  "name": "vue-code-view",
  "version": "0.5.0",
  "packageManager": "pnpm@7.1.0",
  "description": "A Vue 2 component like Vue SFC REPL `@vue/repl` : u can edit, run and preview the code effect display in real time on the web page.",
  "main": "lib/vue-code-viewer.common.js",
  "unpkg": "lib/vue-code-viewer.umd.js",
  "typings": "types/index.d.ts",
  "scripts": {
    "bootstrap": "pnpm install",
    "clean": "rimraf node_modules",
    "dev": "vue-cli-service serve ",
    "dev:play": "vue-cli-service serve --mode play",
    "dist": "rimraf node_modules && pnpm install &&  vue-cli-service build  --mode production --target lib --name vue-code-viewer --dest lib src/index.ts",
    "deploy:build": "vue-cli-service build --mode deploy",
    "lint": "vue-cli-service lint",
    "commit": "cz"
  },
  "dependencies": {
    "codemirror": "^5.62.2",
    "deepmerge": "^4.2.2",
    "pinia": "^2.0.18",
    "pinia-plugin-persistedstate": "^2.1.1",
    "throttle-debounce": "^3.0.1"
  },
  "peerDependencies": {
    "vue": "^2.7.0"
  },
  "devDependencies": {
    "@iconify/vue2": "^1.1.3",
    "@mdit-vue/plugin-headers": "^0.11.1",
    "@types/codemirror": "^5.60.5",
    "@types/jquery": "^3.5.14",
    "@types/node": "^18.0.4",
    "@types/webpack-env": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "@vue/cli-plugin-babel": "~4.5.18",
    "@vue/cli-plugin-eslint": "~4.5.18",
    "@vue/cli-plugin-router": "~4.5.18",
    "@vue/cli-plugin-typescript": "~4.5.18",
    "@vue/cli-service": "~4.5.18",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vueuse/core": "^8.9.4",
    "chalk": "4.1.2",
    "classnames": "^2.3.1",
    "commitizen": "^4.2.5",
    "compression-webpack-plugin": "^6.1.1",
    "core-js": "^3.23.4",
    "css-loader": "^5.2.7",
    "cz-conventional-changelog": "^3.3.0",
    "escape-html": "^1.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^9.2.0",
    "fflate": "^0.7.4",
    "jquery": "^3.6.1",
    "js-base64": "^3.7.3",
    "less": "^4.1.2",
    "markdown-it": "^8.4.1",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-chain": "^1.3.0",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-table-of-contents": "^0.4.4",
    "normalize.css": "^8.0.1",
    "prettier": "^2.2.1",
    "prismjs": "^1.28.0",
    "rimraf": "^3.0.2",
    "sass": "^1.44.0",
    "sass-loader": "^8.0.2",
    "stylus": "^0.58.1",
    "tua-body-scroll-lock": "^1.2.1",
    "typescript": "^4.7.3",
    "vue": "^2.7.0",
    "vue-loader": "^15.10.0",
    "vue-router": "^3.2.0",
    "webpack": "^4.36.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "vue",
    "components",
    "codemirror",
    "code editor"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andurils/vue-code-view.git"
  },
  "author": "andurils",
  "license": "MIT",
  "homepage": "https://andurils.github.io/vue-code-view",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ]
}